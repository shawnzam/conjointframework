<h1>Listing choice_rows</h1>

<table>
  <thead>
    <tr>
      <th>Choice set</th>
      <th>Display order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @choice_rows.each do |choice_row| %>
      <tr>
        <td><%= choice_row.choice_set %></td>
        <td><%= choice_row.display_order %></td>
        <td><%= link_to 'Show', choice_row %></td>
        <td><%= link_to 'Edit', edit_choice_row_path(choice_row) %></td>
        <td><%= link_to 'Destroy', choice_row, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Choice row', new_choice_row_path %>
